{
  "Routes": [
    {
      "UpstreamPathTemplate": "/gateway/products/create-all",
      "UpstreamHttpMethod": [ "Post" ],
      "DownstreamPathTemplate": "/api/products/create-all",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "Product.API",
          "Port": 8080
        }
      ],
      "QoSOptions": {
        "ExceptionsAllowedBeforeBreaking": 3,
        "DurationOfBreak": 10000,
        "TimeoutValue": 5000
      }
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://gatewayapi.dev:8080",
    "RequestIdKey": "OcelotCorrelationId"
  }
}